<!--

This XML defines table dependencies for repository object SECURITY BP ROLE

@version 1.0

-->

<object name="SECURITY BP ROLE">

	<param name="ID_BP"/>

	<param name="ID_APPROLE_BP"/>

	<call-table name="SBP_APPROLE">

		<with-pk name="ID_BP" select="$ID_BP"/>

		<with-pk name="ID_APPROLE_BP" select="$ID_APPROLE_BP"/>

		<read-table name="M4RSC_APPROLE">

			<with-pk name="ID_APP_ROLE" select="$ID_APPROLE_BP"/>

			<call-object name="APP_ROLE">

				<with-param name="ID_APP_ROLE" select="@ID_APP_ROLE"/>

			</call-object>

		</read-table>		

	</call-table>

</object>
